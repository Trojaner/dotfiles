[init]
	defaultBranch = main

[color]
	ui = auto

[color "branch"]

	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]

	meta = yellow bold
	frag = magenta bold # line info
	old = red # deletions
	new = green # additions

[color "status"]

	added = yellow
	changed = green
	untracked = cyan

[diff]

	renames = copies

[url "git@github.com:github"]

	insteadOf = "https://github.com/github"
	insteadOf = "github:github"
	insteadOf = "git://github.com/github"

[url "git@github.com:"]

	pushInsteadOf = "https://github.com/"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"

[url "git://github.com/"]

	insteadOf = "github:"

[url "git@gist.github.com:"]

	insteadOf = "gst:"
	pushInsteadOf = "gist:"
	pushInsteadOf = "git://gist.github.com/"

[url "git://gist.github.com/"]

	insteadOf = "gist:"

[commit]

	gpgsign = true

[pull]
	rebase = true

[user]
	signingkey = ssh-rsa 
	name = Enes Sadık Özbek
	email = es.ozbek@outlook.com

[gpg "ssh"]
	allowedSignersFile = /home/pi/.ssh/allowed_signers

[core]
	editor = nano

[gpg]
	format = ssh
